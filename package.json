{
  "name": "e-market-api",
  "version": "1.0.0",
  "type": "module",
  "description": "La plateforme est une solution e-commerce évolutive conçue pour permettre à l’entreprise de vendre ses produits en ligne de manière simple, sécurisée et performante. Elle offre une interface intuitive pour les utilisateurs ainsi qu’un espace d’administration complet pour la gestion des opérations commerciales.",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node migrations/migrate.js",
    "seed": "node migrations/seed.js",
    "reset": "node migrations/reset-db.js",
    "test": "cross-env test mocha test/setup.js test/**/*.test.js --timeout 5000 --exit",
    "test:unit": "cross-env NODE_ENV=test mocha test/setup.js test/unit/**/*.js --timeout 5000 --exit",
    " ": "cross-env NODE_ENV=test mocha  --no-config test/unit/cartTest.js --timeout 5000 --exit",
    "test:integration:cart": "cross-env NODE_ENV=test mocha  --no-config test/setup.js test/integration/cartTest.js --timeout 5000 --exit",
    "test:unit:cart": "cross-env NODE_ENV=test mocha  --no-config test/setup.js test/unit/cartTest.js --timeout 5000 --exit",
    "test:unit:order": "cross-env NODE_ENV=test mocha  --no-config test/setup.js test/unit/orderTest.js --timeout 5000 --exit",
    "test:unit:coupon": "cross-env NODE_ENV=test mocha  --no-config test/setup.js test/unit/couponTest.js --timeout 5000 --exit",
    "test:integration:coupon": "cross-env NODE_ENV=test mocha  --no-config test/setup.js test/integration/couponTest.js --timeout 20000 --exit",
    "test:integration:order": "cross-env NODE_ENV=test mocha test/setup.js test/integration/orderTest.js --timeout 15000 --exit",
    "test:unit:auth": "cross-env NODE_ENV=test mocha test/setup.js test/unit/authTest.js --timeout 5000 --exit",
    "test:integration": "cross-env NODE_ENV=test mocha test/setup.js test/integration/**/*.js --timeout 10000 --exit",
    "test:integration:auth": "cross-env NODE_ENV=test mocha test/setup.js test/integration/authTest.js --timeout 10000 --exit",
    "test:unit:product": "cross-env NODE_ENV=test mocha --no-config test/unit/productTest.js --timeout 5000 --exit",
    "test:integration:product": "cross-env NODE_ENV=test mocha --no-config test/setup.js test/integration/productTest.js --timeout 10000 --exit",
    "test:unit:notification": "cross-env NODE_ENV=test mocha --no-config test/unit/notificationTest.js --timeout 5000 --exit",
    "test:integration:notification": "cross-env NODE_ENV=test mocha --no-config test/setup.js test/integration/notificationTest.js --timeout 10000 --exit",
    "test:unit:log": "cross-env NODE_ENV=test:unit mocha --no-config test/unit/logTest.js --timeout 5000 --exit",
    "test:integration:log": "cross-env NODE_ENV=test mocha --no-config test/setup.js test/integration/logTest.js --timeout 10000 --exit",
    "coverage": "cross-env NODE_ENV=test c8 --reporter=html --reporter=text --reporter=lcov mocha test/setup.js test/integration/**/*.js --timeout 10000 --exit",
    "mongo": "mongosh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AsforDounia/E-Market-API.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AsforDounia/E-Market-API/issues"
  },
  "homepage": "https://github.com/AsforDounia/E-Market-API#readme",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.0",
    "multer": "^2.0.2",
    "rate-limit-mongo": "^2.3.2",
    "redis": "^5.9.0",
    "sharp": "^0.34.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@faker-js/faker": "^10.1.0",
    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "mocha": "^11.7.4",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4"
  }
}
